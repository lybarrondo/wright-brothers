<krpano>	

<!-- ####  OVERLAYS #### -->
	
		<layer name="mask-welcome" type="container" align="center" width="100%" height="100%" bgcolor="0x000000" bgalpha="0.92" zorder="1" bgcapture="true" 
			keep="true"
			visible="false" 
			handcursor="false"
			onclick="close-mask(mask-welcome);show_layers();hide_guide();set(layer[cont-508].visible,false);"
			/>
			
		<layer name="mask-x" 
			parent="mask-welcome" handcursor="false"
			url="ifg/close-X.png" 
			scale="0.35"
			keep="true" devices="normal" 
			align="topright" x="0" y="0" tooltip="close" 
			onover="set(layer[mask-x].url, ifg/close-XO.png);"
			onout="set(layer[mask-x].url, ifg/close-X.png);"
			onclick="close-mask(mask-welcome);show_layers();hide_guide();set(layer[cont-508].visible,false);"
			/>
			
		<layer name="o-menu" devices="normal"
			parent="mask-welcome" url="ifg/UI-ldr-L.png" align="topleft" edge="topleft" x="240" y="-40"  scale="0.55" alpha="0.0"
			handcursor="false"
			keep="true"
			/>	
			
		<layer name="o-menu-txt" devices="normal"
			parent="o-menu" 
			url="plugins/textfield.swf"  
			html="data:g-menu" css="font-family:DINOT; color:0xEFEFEF; font-size:22px; letter-spacing: 0.05em;"
			align="bottomright" edge="left" x="-2" y="0"  scale="0.6"
			background="false" handcursor="false"
			keep="true"
			/>
			
		<layer name="o-settings" devices="normal"
			parent="mask-welcome" url="ifg/UI-ldr-R.png" align="topright" edge="topright" x="120" y="-160"  scale="0.40" alpha="0.0" handcursor="false"
			tooltip="UI element | intro screen"
			keep="true"
			/>
			
		<layer name="o-settings-btn" devices="normal" handcursor="false"
			parent="o-settings" url="ifg/btn-508-2.png" align="topright" edge="left" x="2" y="0"  scale="0.40" alpha="1.0" 
			keep="true"
			/>
			
		<layer name="o-settings-txt" devices="normal"
			parent="o-settings" handcursor="false"
			url="plugins/textfield.swf"  
			html="data:g-settings" css="font-family:DINOT; color:0xEFEFEF; font-size:22px; letter-spacing: 0.05em;"
			align="bottomleft" edge="right" x="-57" y="0"  scale="0.6"
			background="false"
			keep="true"
			/>

		<layer name="o-title" devices="normal" handcursor="false"
			parent="mask-welcome" url="ifg/UI-ldr-title.png" align="bottom" edge="bottomleft" x="-224" y="40"  scale="0.55" alpha="0.0"
			tooltip="UI element | intro screen"
			keep="true"
			/>
			
		<layer name="o-title-txt" devices="normal"
			parent="o-title" handcursor="false"
			url="plugins/textfield.swf"  
